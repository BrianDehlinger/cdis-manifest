{%- set sower_enabled = True -%}
{%- set sower_memory_limit = '4Gi' -%}
{%- set hatchery_enabled = True -%}
{%- set kube_namespace = 'staging' -%}
{%- set vpc_name = 'stagingdatastage' -%}
{%- set hatchery_user_namespace = 'jupyter-pods-staging' -%}
{% extends "external-staging/external_staging_manifest.json" %}

  {% block versions %}
    "arborist": "quay.io/cdis/arborist:2.4.0",
    "fence": "quay.io/cdis/fence:4.13.4",
    "indexd": "quay.io/cdis/indexd:2.6.1",
    "peregrine": "quay.io/cdis/peregrine:3.0.0",
    "pidgin": "quay.io/cdis/pidgin:1.0.0",
    "sheepdog": "quay.io/cdis/sheepdog:3.0.0",
    "portal": "quay.io/cdis/data-portal:2.24.3",
    "spark": "quay.io/cdis/gen3-spark:1.0.0",
    "tube": "quay.io/cdis/tube:0.3.18",
    "guppy": "quay.io/cdis/guppy:0.3.14",
    "sower": "quay.io/cdis/sower:0.3.0",
    "hatchery": "quay.io/cdis/hatchery:0.1.0",
    "wts": "quay.io/cdis/workspace-token-service:0.2.0",
    "manifestservice": "quay.io/cdis/manifestservice:0.2.0",
    {% if super %}{{ super() }}{% endif %}
  {% endblock %}

  {% block global %}
    "hostname": "{{ hostname }}",
    "revproxy_arn": "arn:aws:acm:us-east-1:895962626746:certificate/1876fa51-031e-4aa9-ab95-7da182a52c7f",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/gtexdictionary/3.3.9/schema.json",
    "kube_bucket": "kube-stageprod-gen3",
    "logs_bucket": "logs-stageprod-gen3",
    "sync_from_dbgap": "False",
    "useryaml_s3path": "s3://cdis-gen3-users/stagingdatastage/user.yaml",
    {{ super() }}
  {% endblock %}

  {% block guppy %}
    "indices": [
      {
        "index": "dcp_etl",
        "type": "subject"
      },
      {
        "index": "dcp_file",
        "type": "file"
      }
    ],
    "config_index": "dcp_array-config",
    {{ super() }}
  {% endblock %}
